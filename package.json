{
  "name": "react-jmtable",
  "version": "0.2.35",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "postcss.config.js"
  ],
  "keywords": [
    "table",
    "react-table"
  ],
  "author": "Jae M. Choi",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.59.2",
    "sass-loader": "^13.2.0",
    "styled-components": "^5.3.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:css": "tailwind build src/styles.css -o build/styles.css && purgecss --config purgecss.config.js --out build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "rm -rf dist && mkdir dist && babel src/lib -d dist --copy-files",
    "purgecss": "purgecss --config purgecss.config.js --css dist/index.css --output dist/index.css",
    "publish:npm": "yarn build && yarn purgecss"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/preset-react": "^7.18.6"
  }
}
